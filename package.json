    "husky": "^0.14.3",
    "interpolate-html-plugin": "^3.0.0",
    "lint-staged": "^7.1.3",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "1.13.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-stylish": "^0.1.8"
  },
  "dependencies": {
    "autoprefixer": "^9.2.0",
    "elm": "^0.19.0-bugfix2",
    "elm-format": "^0.8.1",
    "fuse-box": "^3.3.0-next.9",
    "fuse-box-elm-plugin": "^1.1.1",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "postcss": "^7.0.5",
    "postcss-flexbugs-fixes": "^3.3.0",
    "purgecss": "^1.0.1",
    "tailwindcss": "^0.6.6",
    "typescript": "^3.1.3",
    "uglify-js": "^3.3.5",
    "workbox-build": "^3.2.0"
  },
  "scripts": {
    "start": "node fuse.js front-dev",
    "prod": "node fuse.js front-prod",
    "debug-sw": "node fuse.js front-debug-sw",
    "precommit": "lint-staged",
    "webpack-dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --quiet --color -d --config webpack.config.js",
    "webpack": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --display none -p --config webpack.config.js"
  },
  "browserslist": [
    ">= 2%"
  ],
  "lint-staged": {
    "src/**/*.elm": [
      "./node_modules/.bin/elm-format src --yes",
      "git add"
    ],
    "src/**/*.{ts,js,scss}": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  }
}
