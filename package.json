{
  "name": "portfolio",
  "version": "2.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --quiet --color -d --config webpack.config.ts",
    "build": "cross-env NODE_ENV=production USE_SW=true ts-node -T ./scripts/build.ts",
    "build-check": "yarn build && ts-node ./scripts/mock-server.ts",
    "build-netlify": "cross-env NODE_ENV=production NETLIFY=true ts-node -T ./scripts/build.ts",
    "check-deps": "yarn outdated && yarn audit",
    "debug-sw": "node fuse.js front-debug-sw"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.elm": [
      "./node_modules/.bin/elm-format --yes",
      "git add"
    ],
    "**/*.{ts,scss}": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@dillonkearns/elm-graphql": "^3.2.0",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.10.5",
    "@types/webpack": "^4.4.25",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "morgan": "^1.9.1",
    "prettier": "latest",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "autoprefixer": "^9.4.2",
    "copy-webpack-plugin": "^5.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "elm": "^0.19.0-bugfix6",
    "elm-format": "^0.8.1",
    "elm-webpack-loader": "^5.0.0",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "html-minifier": "^3.5.21",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "interpolate-html-plugin": "^3.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.6",
    "postcss-loader": "^3.0.0",
    "purgecss": "^1.1.0",
    "purgecss-webpack-plugin": "^1.4.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^0.7.4",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-stylish": "^0.1.8",
    "workbox-build": "^4.0.0"
  }
}
